---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Mapiator</title>
    <link href="http://feeds2.feedburner.com/....." rel="self"/>
    <link href="http://ept.github.com/invoicing/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://pdietrich.github.com/mapiator/</id>
    <author>
        <name>Patrick Dietrich</name>
        <email>mapiator@pdietrich.net</email>
    </author>

    {% for post in site.posts %}
        <entry>
            <title>{{ post.title }}</title>
            <link href="http://pdietrich.github.com/mapiator{{ post.url }}"/>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <id>http://pdietrich.github.com/mapiator{{ post.id }}</id>
            <content type="html">{{ post.content | xml_escape }}</content>
        </entry>
    {% endfor %}
</feed>
